# Supabase Environment Variables
# Copy this file to .env and fill in your values

# Local Supabase URL (standard for local development)
SUPABASE_URL=http://127.0.0.1:54321

# Local Supabase Anon Key (get this from the Supabase Dashboard or CLI output)
SUPABASE_ANON_KEY=your-anon-key-here

# The user ID of the teacher account (replace with your actual user ID)
# This is used for data migration to correctly assign ownership
SUPABASE_USER_ID=your-user-id-here

# Other application variables
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here 